{
  "name": "React Extensions",
  "description": "Base Level Extension",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "tabs",
    "activeTab",
    "scripting",
    "identity"
  ],
  "oauth2": {
    "client_id": "782545755178-hq387gkj6n7naccc1sf19mfuvmeg93c0.apps.googleusercontent.com",
    "scopes": [
      "email",
      "profile"
    ],
    "response_type": "token",
    "redirect_uri": "https://hkkidcmeagmplkgfjafhlckeckmodipj.chromiumapp.org/oauth2"
  },
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'; script-src-elem 'self' 'unsafe-inline' https://music.yandex.ru/;"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": "logo192.png"
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+Shift+F",
        "mac": "MacCtrl+Shift+F"
      },
      "description": "Opens index.html"
    },
    "show-modal": {
      "suggested_key": {
        "default": "Ctrl+Shift+Y",
        "mac": "MacCtrl+Shift+Y"
      },
      "description": "Show modal with selected text"
    }
  },
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "contentScript.js"
      ],
      "css": [
        "static/css/main.min.css"
      ]
    }
  ]
}